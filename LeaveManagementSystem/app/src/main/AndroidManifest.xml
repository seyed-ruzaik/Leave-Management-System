<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.leavemanagementsystem">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.LeaveManagementSystem">
        <activity android:name=".Splash_Screen"></activity>
        <activity android:name=".Leaves_For_Jane" />
        <activity android:name=".Leaves_For_John_activity" />
        <activity android:name=".Jane_Doe_activity" />
        <activity android:name=".John_Doe_activity" />
        <activity android:name=".Approve_And_Reject_activity" />
        <activity android:name=".View_Leave_Balances_activity" />
        <activity android:name=".Manager_Menu_activity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>